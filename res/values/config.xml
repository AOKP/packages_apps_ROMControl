<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Whether or not to display the notification LED settings -->
    <bool name="has_notification_led">true</bool>

    <!-- Whether or not to display the Spen settings -->
    <bool name="config_stylusGestures">false</bool>

    <!-- Whether device has camera flash or torch capabilities -->
    <bool name="has_torch">true</bool>

    <!-- Whether device has hardware buttons -->
    <bool name="has_hardware_buttons">true</bool>

    <!-- Whether can use fast chage -->
    <bool name="has_fast_charge">true</bool>

    <!-- Whether can use color tuning -->
    <bool name="has_color_tuning">true</bool>

    <!-- Whether device has led brightness feature -->
    <bool name="has_led_brightness_feature">false</bool>

    <!-- Whether device has led charging feature -->
    <bool name="has_led_charging_feature">false</bool>

    <!-- If there is no sysfs-based control mechanism, enable this -->
    <bool name="useCameraInterface">true</bool>

    <!-- Full path to the sysfs toggle -->
    <string name="flashDevice">/sys/class/leds/flashlight/brightness</string>

    <!-- Full path to the sysfs luminosity -->
    <string name="flashDeviceLuminosity"></string>

    <!-- Off setting -->
    <integer name="valueOff">0</integer>

    <!-- On setting -->
    <integer name="valueOn">1</integer>

    <!-- Low setting -->
    <integer name="valueLow">50</integer>

    <!-- Middle setting -->
    <integer name="valueHigh">128</integer>

    <!-- Highest setting, leave -1 if it doesn't exist -->
    <integer name="valueDeathRay">3</integer>
</resources>
