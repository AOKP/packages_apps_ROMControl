<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

        <CheckBoxPreference
            android:key="enable_softkey"
            android:title= "@string/softkey_enable"
            android:summary= "@string/softkey_enable_summary"  />

        <CheckBoxPreference
            android:key="enable_softkey_double_click"
