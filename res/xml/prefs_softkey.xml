            android:title= "@string/softkey_enable_double_click"
            android:summary= "@string/softkey_enable_double_click_summary"  />

        <ListPreference
            android:key="softkey_home_short"
            android:dependency="enable_softkey"
            android:title="@string/softkey_home_short_title"
            android:entries="@array/softkey_button_entries"
            android:entryValues="@array/softkey_button_values" />

