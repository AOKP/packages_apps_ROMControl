<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android">

    <Preference
        android:key="baked_website"
        android:title="@string/baked_website_title"
        android:summary="@string/baked_website_summary" />

    <Preference
        android:key="baked_source"
        android:title="@string/baked_source_title"
        android:summary="@string/baked_source_summary" />

    <Preference
        android:key="baked_irc"
        android:title="@string/baked_irc_title"
        android:summary="@string/baked_irc_summary" />

    <Preference
        android:id="@+id/aboutpage"
        android:title="@string/baked_devs"
        android:summary="@string/baked_devs_summary">

        <intent
                android:action="android.intent.action.MAIN"
                android:targetClass="com.baked.romcontrol.AboutActivity"
                android:targetPackage="com.baked.romcontrol" />

    </Preference>
</PreferenceScreen>
